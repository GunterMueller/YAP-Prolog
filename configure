         then
           if test "$debugyap" = "yes"
           then
             CFLAGS="-Ae -g"
             C_INTERF_FLAGS="-Ae -g"
           else
             CFLAGS="-Ae -O3"
             C_INTERF_FLAGS="-Ae"
           fi
        fi
        ;;
       hppa*)
        if test "$CC" = "cc" -o $CC = c89
         then
           if test "$debugyap" = "yes"
           then
             CFLAGS="-g"
             C_INTERF_FLAGS="-g"
           else
             CFLAGS="-Ae +O4"
           fi
        fi
        ;;
     esac
    fi
    C_INTERF_FLAGS="$CFLAGS"
fi

