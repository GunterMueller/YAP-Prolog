        <name>GECODE_INT_HALL_ONE</name>
        <param><defname>x0</defname></param>
        <param><defname>x1</defname></param>
        <param><defname>x2</defname></param>
        <initializer>if (x0.assigned()) {                          \
    GECODE_ME_CHECK(x1.nq(home,x0.val()));      \
    GECODE_ME_CHECK(x2.nq(home,x0.val()));      \
    if (x1.assigned()) {                        \
      GECODE_ME_CHECK(x2.nq(home,x1.val()));    \
      return home.ES_SUBSUMED(*this);            \
    }                                           \
    if (x2.assigned()) {                        \
      GECODE_ME_CHECK(x1.nq(home,x2.val()));    \
      return home.ES_SUBSUMED(*this);            \
    }                                           \
    return ES_FIX;                              \
  }</initializer>
