      <memberdef kind="define" id="ter-dom_8hpp_1aec608b41cc122be54370e92390156aa3" prot="public" static="no">
        <name>GECODE_INT_HALL_TWO</name>
        <param><defname>x0</defname></param>
        <param><defname>x1</defname></param>
        <param><defname>x2</defname></param>
        <initializer>if ((x0.size() == 2) &amp;&amp; (x1.size() == 2) &amp;&amp;                   \
      (x0.min() == x1.min()) &amp;&amp; (x0.max() == x1.max())) {       \
    GECODE_ME_CHECK(x2.nq(home,x0.min()));                      \
    GECODE_ME_CHECK(x2.nq(home,x0.max()));                      \
    return ES_FIX;                                              \
  }</initializer>
